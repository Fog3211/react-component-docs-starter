{
  "name": "react-component-docs-starter",
  "version": "1.0.0",
  "description": "A documentation sample of a React component library made with Storybook",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "prebuild": "rimraf dist",
    "build:docs": "storybook build",
    "build:type": "tsc",
    "build:assets": "copyfiles src/**/*.* -e \" **/*.{tsx,ts,js,jsx} \" -a -u 1 dist",
    "build:component": "swc src -d dist --source-maps",
    "build": "npm run build:type && npm run build:component && npm run build:assets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fog3211/react-component-docs-starter.git"
  },
  "keywords": [
    "stroybook",
    "react",
    "typescript",
    "vite",
    "swc"
  ],
  "author": "Fog3211",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.22",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/builder-vite": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "@storybook/react-vite": "^7.0.22",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@types/react": "^18.2.12",
    "copyfiles": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "storybook": "^7.0.22",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  }
}
